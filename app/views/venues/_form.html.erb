<%= simple_form_for(@venue) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :venuename %>
    <%= f.input :address1 %>
    <%= f.input :address2 %>
    <%= f.input :addresstown %>
    <%= f.input :addresscounty %>
    <%= f.input :postcode %>
    <%= f.input :email %>
    <%= f.input :phone %>
    <%= f.input :website %>
    <%= f.input :ddaaccess %>
    <%= f.input :computers %>
    <%= f.input :toilets %>
    <%= f.input :wifi %>
    <%= f.input :rentalcost %>
    <%= f.input :otherinfo %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
